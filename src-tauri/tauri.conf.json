{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ssh-config-enhance",
  "version": "0.1.0",
  "identifier": "com.example.ssh-config-enhance",
  "build": {
    "frontendDist": "../web/dist",
    "devUrl": "http://127.0.0.1:5005",
    "beforeDevCommand": "cd ../web && npm run dev",
    "beforeBuildCommand": "cd ../web && npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "SSH配置管理器",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png"
    ]
  }
}